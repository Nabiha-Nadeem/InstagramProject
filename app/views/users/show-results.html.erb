<div class="box-center">
  <% users.each do |user| %>
    <% if user != current_user %>
        <div class="row search-result">
          <%= image_tag(user.avatar, class: "thumbnail-avatar") %>
          <%= link_to user.fullname, user_path(user.id), class: "btn post-username" %>
        </div>
    <% end %>
  <% end %>
</div>